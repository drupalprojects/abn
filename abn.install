/**
 * Implements hook_field_schema().
 */
function abn_field_schema($field) {
  return array(
    'columns' => array(
      'url' => array(
        'type' => 'varchar',
        'length' => 2048, // Maximum URLs length.
        'not null' => FALSE,
        'sortable' => TRUE
      ),
    ),
  );
}
